{
	"name": "ghost-search",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "vue-cli-service serve",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"build.wc": "vue-cli-service build --target wc --name ghost-search",
		"build.wc-async": "vue-cli-service build --target wc-async --name ghost-search",
