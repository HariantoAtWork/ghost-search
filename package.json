		"dayjs": "^1.8.23",
		"md5": "^2.2.1",
		"vue": "^2.6.11"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.2.0",
		"@vue/cli-plugin-eslint": "~4.2.0",
		"@vue/cli-service": "~4.2.0",
		"babel-eslint": "^10.0.3",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.1.2",
		"node-sass": "^4.13.1",
		"sass-loader": "^8.0.2",
		"vue-template-compiler": "^2.6.11"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential"
